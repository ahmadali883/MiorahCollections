    "bcryptjs": "^2.4.3",
    "config": "^3.3.7",
    "csrf": "^3.1.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.0",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "multer": "^1.4.5-lts.2",

    "test:auth-flow": "node test/authFlowTest.js",
    "test:checkout-validation": "node test/checkoutValidationTest.js",
    "test:token-management": "node test/tokenManagementTest.js",
    "test:cart-sync": "node test/cartSynchronizationTest.js"
  },
